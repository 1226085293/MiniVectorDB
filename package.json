{
	"name": "mini-vector-db",
	"version": "1.0.2",
	"description": "MiniVectorDB with HNSW (AssemblyScript WASM) + Local embedder",
	"repository": "github:1226085293/MiniVectorDB",
	"keywords": [
		"vector-database",
		"vectordb",
		"vector-search",
		"embedding",
		"semantic-search",
		"ann",
		"hnsw",
		"approximate-nearest-neighbor",
		"wasm",
		"webassembly",
		"simd",
		"int8",
		"quantization",
		"rerank",
		"exact-search",
		"nodejs",
		"local-database",
		"file-based",
		"self-hosted",
		"offline"
	],
	"license": "MIT",
	"type": "commonjs",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"require": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"files": [
		"dist/**/*",
		"README.md",
		"LICENSE",
		"package.json"
	],
	"scripts": {
		"test": "ts-node tests/test.ts",
		"build": "npm run clean && npm run build:wasm && npm run build:ts && npm run copy:wasm",
		"clean": "node -e \"require('fs').rmSync('dist',{recursive:true,force:true}); require('fs').rmSync('build',{recursive:true,force:true});\"",
		"build:wasm": "npx asc assembly/index.ts --config asconfig.json --target release",
		"build:ts": "npx tsc -p tsconfig.json",
		"copy:wasm": "node -e \"const fs=require('fs'); const path=require('path'); fs.mkdirSync('dist',{recursive:true}); fs.copyFileSync(path.join('build','release.wasm'), path.join('dist','release.wasm'));\"",
		"pretest": "npm run build:wasm",
		"prepack": "npm run build",
		"generate-md": "node mdg -e ts json -d ./assembly ./src ./asconfig.json ./tsconfig.json",
		"generate-test-md": "node mdg -e ts -d ./tests"
	},
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"@assemblyscript/loader": "^0.27.0",
		"@xenova/transformers": "^2.16.0",
		"fastify": "^5.0.0",
		"@fastify/cors": "^10.0.0",
		"lokijs": "^1.5.12"
	},
	"devDependencies": {
		"dotenv": "^17.2.4",
		"@types/lokijs": "^1.5.7",
		"@types/node": "^18.0.0",
		"assemblyscript": "^0.27.0",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.0"
	}
}
